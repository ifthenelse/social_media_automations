{
    "venvPath": ".",
    "venv": "venv",
    "executionEnvironments": [
        {
            "root": "./src",
            "pythonVersion": "3.13",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                "./venv/lib/python3.13/site-packages"
            ]
        }
    ],
    "include": [
        "src/**",
        "*.py"
    ],
    "exclude": [
        "venv/**",
        "__pycache__/**"
    ]
}
